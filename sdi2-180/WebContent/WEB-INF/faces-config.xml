			<from-outcome>registrarse</from-outcome>
			<to-view-id>/registrarse.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	
	
	
