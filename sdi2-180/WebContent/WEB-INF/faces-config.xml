    <application>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>
	
	<managed-bean> <!-- se declaran aqui el bean para hacer que codigo sea lo mas auto-documentado -->
		<managed-bean-name>settings</managed-bean-name>
		<managed-bean-class>com.sdi.presentation.BeanSettings</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registrarse</managed-bean-name>
		<managed-bean-class>com.sdi.presentation.BeanRegistrarse</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>com.sdi.presentation.BeanLogin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viajes</managed-bean-name>
		<managed-bean-class>com.sdi.presentation.BeanViajes</managed-bean-class>
