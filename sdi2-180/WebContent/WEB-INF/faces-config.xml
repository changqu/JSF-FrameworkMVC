	<managed-bean>
		<managed-bean-name>ciudadProvincia</managed-bean-name>
		<managed-bean-class>com.sdi.presentation.BeanCiudadProvincia</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registrarse</from-outcome>
			<to-view-id>/registrarse.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listaViajes</from-outcome>
			<to-view-id>/listaViajesPublico.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-action>#{login.verify}</from-action>
			<from-outcome>exito</from-outcome>
			<to-view-id>/registrado/principal.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-action>#{login.verify}</from-action>
			<from-outcome>error</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/registrarse.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listaViajes</from-outcome>
			<to-view-id>/listaViajesPublico.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
